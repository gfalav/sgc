<h1>Listing contratos</h1>

<table>
  <thead>
    <tr>
      <th>Cuenta</th>
      <th>Nombre</th>
      <th>Tcuit</th>
      <th>Tiibb</th>
      <th>Tperfact</th>
      <th>Econtrato</th>
      <th>Fcontratoact</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contratos.each do |contrato| %>
      <tr>
        <td><%= contrato.cuenta_id %></td>
        <td><%= contrato.nombre %></td>
        <td><%= contrato.tcuit_id %></td>
        <td><%= contrato.tiibb_id %></td>
        <td><%= contrato.tperfact_id %></td>
        <td><%= contrato.econtrato_id %></td>
        <td><%= contrato.fcontratoact %></td>
        <td><%= link_to 'Show', contrato %></td>
        <td><%= link_to 'Edit', edit_contrato_path(contrato) %></td>
        <td><%= link_to 'Destroy', contrato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contrato', new_contrato_path %>
