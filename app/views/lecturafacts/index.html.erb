<h1>Listing lecturafacts</h1>

<table>
  <thead>
    <tr>
      <th>Lectura</th>
      <th>Factura</th>
      <th>Medidor</th>
      <th>Tlectura</th>
      <th>Flectura</th>
      <th>Tconsumo</th>
      <th>Lectura</th>
      <th>Ctelect</th>
      <th>Consumo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lecturafacts.each do |lecturafact| %>
      <tr>
        <td><%= lecturafact.lectura_id %></td>
        <td><%= lecturafact.factura_id %></td>
        <td><%= lecturafact.medidor_id %></td>
        <td><%= lecturafact.tlectura_id %></td>
        <td><%= lecturafact.flectura %></td>
        <td><%= lecturafact.tconsumo_id %></td>
        <td><%= lecturafact.lectura %></td>
        <td><%= lecturafact.ctelect %></td>
        <td><%= lecturafact.consumo %></td>
        <td><%= link_to 'Show', lecturafact %></td>
        <td><%= link_to 'Edit', edit_lecturafact_path(lecturafact) %></td>
        <td><%= link_to 'Destroy', lecturafact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lecturafact', new_lecturafact_path %>
