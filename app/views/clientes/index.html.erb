<h1>Listing clientes</h1>

<table>
  <thead>
    <tr>
      <th>Apellido</th>
      <th>Nombres</th>
      <th>Documento</th>
      <th>Tdoc</th>
      <th>Iibb</th>
      <th>Tiibb</th>
      <th>Telefono</th>
      <th>Email</th>
      <th>Weblog</th>
      <th>Contacto</th>
      <th>Ciiu</th>
      <th>Tcliente</th>
      <th>Direccion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.apellido %></td>
        <td><%= cliente.nombres %></td>
        <td><%= cliente.documento %></td>
        <td><%= cliente.tdoc_id %></td>
        <td><%= cliente.iibb %></td>
        <td><%= cliente.tiibb %></td>
        <td><%= cliente.telefono %></td>
        <td><%= cliente.email %></td>
        <td><%= cliente.weblog %></td>
        <td><%= cliente.contacto %></td>
        <td><%= cliente.ciiu_id %></td>
        <td><%= cliente.tcliente_id %></td>
        <td><%= cliente.direccion_id %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
